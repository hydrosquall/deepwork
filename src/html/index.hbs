<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>

	{{> base/meta}}

	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}

</head>

<body>

	{{> base/header 'sticky'}}

	<main id='content'>
		{{> story/intro }}
		{{> story/feature-block which='resume' title=content.resume.title }}
		{{> story/feature-block which='photo-studio' title=content.photo.title }}
		{{> story/feature-block which='thought-leadership' title=content.thought-leadership.title }}
		{{> story/feature-block which='video-call' title=content.video-call.title }}

	</main>

	{{> base/footer }}


	<script src='{{basepath}}assets/scripts/d3.v6.0.0.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>