<!doctype html>
<html lang='en' data-version='{{timestamp}}'>
<!-- TODO - Limit characters imported from bold just to those needed -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=DM+Sans:wght@400;500;700&display=swap" rel="stylesheet">

<head>

	{{> base/meta}}

	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}

</head>

<body>


	<main id='content'>
		{{> story/intro }}
		{{> story/feature-block which='resume' content=content.resume}}
		{{> story/feature-block which='photo-studio' content=content.photo}}
		{{> story/feature-block which='photo-studio-b' content=content.photo-b}}
		{{> story/feature-block which='thought-leadership' content=content.thought-leadership}}
		{{> story/feature-block which='video-call' content=content.video-call}}
		{{> story/outro content=content.outro}}
		{{> story/reviews reviews=content.reviews}}
	</main>

	{{> base/footer }}


	<script src='{{basepath}}assets/scripts/d3.v6.0.0.min.js'></script>
	<script src='bundle.js?version={{timestamp}}' async></script>
</body>

</html>